package ui;

import javax.swing.*;
import java.awt.*;

/*
 * ManageEquipmentFrame
 * Admin functionality:
 * - Add and view equipment
 */
public class ManageEquipmentFrame extends JFrame {

    private static final long serialVersionUID = 1L;

    public ManageEquipmentFrame() {
        setTitle("Manage Equipment");
        setSize(500, 300);
        setLocationRelativeTo(null);

        JLabel label = new JLabel("Manage Equipment Screen", JLabel.CENTER);
        label.setFont(new Font("Arial", Font.BOLD, 16));

        add(label);
    }
}
